{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "helpers",
      "libs",
      "manager",
      "middleware",
      "templateExtends"
    ],
    "exclude": [
      "apidocs",
      "config",
      "docs",
      "example",
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName": "Arrowjs Core",
    "footer": "Developed by Techmaster Vietnam",
    "copyright": "MIT license",
    "navType": "vertical",
    "theme": "united",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "dateFormat": "YYYY MM DD",
    "highlightTutorialCode": true,
    "syntaxTheme": "dark"
  }
}